{
  "meta": {
    "description": "TSB Admin",
    "viewport": "width=device-width, initial-scale=1",
    "robots": "noindex,nofollow"
  },
  "url": {
    "base": "https://tsb-metadata-generator.herokuapp.com/api/v1",
    "locations": {
      "base": "/locations",
      "export": "/export",
      "import": "/import"
    },
    "tags": "/tags",
    "user": {
      "base": "/users",
      "login": "/login",
      "refreshToken": "/refreshToken"
    },
    "upload": "/files"
  },
  "localStoragePrefix": "tsb",
  "types": ["organisation", "venue"],
  "accessibility": [
    {"label": "Ja", "value": "yes"},
    {"label": "Nein", "value": "no"},
    {"label": "Teilweise", "value": "limited"},
    {"label": "Unbekannt", "value": "unknown"}
  ],
  "nav": [
    {
      "label": "Kulturorte",
      "url": "/",
      "roles": ["ADMIN", "USER", "ANONYMOUS"]
    },
    {
      "label": "Metadaten Generator",
      "url": "/metadaten",
      "roles": ["ADMIN", "USER", "ANONYMOUS"]
    },
    {
      "label": "Kategorien",
      "url": "/tags",
      "roles": ["ADMIN"]
    },
    {
      "label": "Einstellungen",
      "url": "/einstellungen",
      "roles": ["ADMIN", "USER"]
    }, {
      "label": "Nutzer",
      "url": "/nutzer",
      "roles": ["ADMIN"]
    }
  ]
}
